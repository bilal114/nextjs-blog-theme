{
  "name": "gerald-website",
  "description": "Repository for Gerald website",
  "version": "0.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.js",
    "lint:js": "./node_modules/.bin/eslint \"src/**/*.js\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "build:stage": "cross-env NODE_ENV=stage webpack --config ./config/webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "prettier": "npx prettier --config ./.prettierrc --write '**/*.{ts,tsx,scss,js,json,css,html}'"
  },
  "devDependencies": {
    "@babel/cli": "~7.12.0",
    "@babel/core": "~7.12.0",
    "@babel/preset-env": "~7.12.0",
    "autoprefixer": "~10.2.0",
    "babel-loader": "~8.2.0",
    "clean-webpack-plugin": "~3.0.0",
    "copy-webpack-plugin": "^6.0.0",
    "cross-env": "~7.0.0",
    "css-loader": "~5.0.0",
    "cssnano": "~4.1.0",
    "eslint": "~7.17.0",
    "eslint-config-airbnb-base": "~14.2.0",
    "eslint-config-prettier": "~7.1.0",
    "eslint-loader": "~4.0.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-prettier": "~3.3.0",
    "favicons-webpack-plugin": "~4.2.0",
    "file-loader": "~6.2.0",
    "glob": "~7.1.0",
    "html-loader": "~1.3.0",
