    "postcss-sass": "~0.4.0",
    "prettier": "~2.2.0",
    "react-dev-utils": "~11.0.0",
    "robotstxt-webpack-plugin": "~7.0.0",
    "sass-loader": "~10.1.0",
    "sitemap-webpack-plugin": "~1.0.0",
    "style-loader": "~2.0.0",
    "stylelint": "~13.8.0",
    "stylelint-config-standard": "~20.0.0",
    "stylelint-scss": "~3.18.0",
    "stylelint-webpack-plugin": "~2.1.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.0.0",
    "webpackbar": "^4.0.0"
